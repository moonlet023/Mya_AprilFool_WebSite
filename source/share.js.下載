(function(i,n,o){i(function(){i(".community_sharebtn li").not(".copyBtn").click(function(e){let t=i(this).data("url"),a=i(this).data("to");l.share(t,a)}),i(".community_sharebtn .copyBtn").click(function(e){l.copy()})});let l={ga:{isSet:!1,category:"",action:"",label:""},copy:function(){let e=i("#copyUrl");if(e.attr("type","text"),navigator.userAgent.match(/iPhone|iPod|iPad/i)){let t=e.get(0);t.contentEditable=!0,t.readOnly=!1;let a=document.createRange();a.selectNodeContents(t);let c=n.getSelection();c.removeAllRanges(),c.addRange(a),t.setSelectionRange(0,999999),t.contentEditable=!1,t.readOnly=!0}else e.select();navigator.clipboard.writeText(e.val()).then(()=>{Dialogify.alert("\u8907\u88FD\u6210\u529F")},()=>{Dialogify.alert("\u8907\u88FD\u5931\u6557")}),e.attr("type","hidden")},share:function(e,t){let a="left=200,top=200,width=720,height=530,scrollbars=1";if(this.ga.isSet)switch(t){case"wall":this.ga.action="\u5206\u4EAB\u5DF4\u54C8";break;case"fb":this.ga.action="\u5206\u4EABfb";break;case"line":this.ga.action="\u5206\u4EABline";break;case"twitter":this.ga.action="\u5206\u4EABtwitter";break;case"plurk":this.ga.action="\u5206\u4EABplurk";break;case"im":this.ga.action="\u5206\u4EABim";break;default:break}n.open(e,"",a)},setGa:function(e,t,a){this.ga.category=e,this.ga.action=t,this.ga.label=a,this.ga.isSet=!0}};n.Share=l})(jQuery,window);
